{
  "name": "qaap-kickstart",
  "version": "1.0.2",
  "description": "qaap microservice boilerplate in ECMAScript",
  "main": "dist/index.js",
  "bin": {
    "kickstart": "./bin/kickstart"
  },
  "scripts": {
    "pretest": "mkdir -p dist && bash ./tools/beautify_source.sh && ./node_modules/babel-cli/bin/babel.js src -d ./dist",
    "test": "./node_modules/mocha/bin/mocha test/index.spec.js",
    "prestart": "mkdir -p dist && bash ./tools/beautify_source.sh && ./node_modules/babel-cli/bin/babel.js src -d ./dist",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/qaap/kickstart.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/qaap/kickstart.git",
  "dependencies": {
    "amqplib": "^0.4.2",
    "assert-args": "^1.1.2",
    "bluebird": "^3.3.4",
    "bristol": "^0.3.3",
    "json-stringify-safe": "^5.0.1",
    "rethinkdb": "^2.3.2",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-plugin-transform-async-to-generator": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-plugin-uglify": "^1.0.2",
    "babel-preset-es2015": "^6.6.0",
    "dockerode": "^2.2.10",
    "js-beautify": "^1.6.2",
    "mocha": "^2.5.3"
  },
  "engines": {
    "node": ">=6.2.2"
  }
}
